<?xml version='1.0' encoding='utf-8'?>
<widget id="com.currency.io" version="1.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Crypto Currency Converter</name>
    <description>
        Simple and easy to convert crypto to selected currency.
    </description>
    <author email="devkhalil1337@gmail.com" href="#">
        Khalil
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="loadUrlTimeoutValue" value="700000" />
</widget>
